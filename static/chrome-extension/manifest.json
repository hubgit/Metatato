{
  "name": "Metatato",
  "description": "A web interface to your Mendeley library",
  "version": "1.3.2",
  "app": {
    "urls": ["http://metatato.com/"],
    "launch": { "web_url": "http://metatato.com/mendeley/" }
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": ["unlimitedStorage"],
  "container": "GOOGLE_DRIVE",
  "api_console_project_id": "420296899738",
  "intents": {
    "http://webintents.org/save": [
      {
        "type": ["text/uri-list"],
        "href": "http://metatato.com/mendeley/static/intents/save.html",
        "title" : "Save To Mendeley",
        "disposition" : "window"
      }
    ],
    "http://webintents.org/view": [
      {
        "type": ["application/pdf"],
        "href": "http://metatato.com/mendeley/static/intents/view.html",
        "title" : "View in Metatato",
        "disposition" : "window"
      }
    ]
  },
  "offline_enabled": true
}

